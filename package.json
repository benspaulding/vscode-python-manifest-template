{
    "name": "python-manifest-template",
    "displayName": "Python MANIFEST.in",
    "description": "Grammar & snippets for Python MANIFEST.in template files.",
    "license": "BSD-3-Clause",
    "version": "0.0.1",
    "publisher": "benspaulding",
    "author": {
        "name": "Ben Spaulding"
    },
    "engines": {
        "vscode": "^1.29.0"
    },
    "keywords": [
        "python",
        "manifest"
